\"\n        autoFocus\n      />\n      <br />\n      <button type=\"submit\" className=\"btn btn-raised\">\n        Register\n      </button>\n    </form>\n  );\n\n  return (\n    <div className=\"container p-5\">\n      <div className=\"row\">\n        <div className=\"col-md-6 offset-md-3\">\n          <h4>Register</h4>\n          {registerForm()}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Register;\n","/home/thanhphat/SportShop-Web-Frontend/src/components/auth/login.js",["41"],"/home/thanhphat/SportShop-Web-Frontend/src/components/auth/forgotPassword.js",["42"],{"ruleId":"43","severity":1,"message":"44","line":35,"column":8,"nodeType":"45","endLine":35,"endColumn":10,"suggestions":"46"},{"ruleId":"43","severity":1,"message":"47","line":12,"column":6,"nodeType":"45","endLine":12,"endColumn":12,"suggestions":"48"},{"ruleId":"43","severity":1,"message":"47","line":17,"column":6,"nodeType":"45","endLine":17,"endColumn":12,"suggestions":"49"},{"ruleId":"43","severity":1,"message":"47","line":13,"column":8,"nodeType":"45","endLine":13,"endColumn":14,"suggestions":"50"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["51"],"React Hook useEffect has a missing dependency: 'history'. Either include it or remove the dependency array.",["52"],["53"],["54"],{"desc":"55","fix":"56"},{"desc":"57","fix":"58"},{"desc":"57","fix":"59"},{"desc":"57","fix":"60"},"Update the dependencies array to be: [dispatch]",{"range":"61","text":"62"},"Update the dependencies array to be: [history, user]",{"range":"63","text":"64"},{"range":"65","text":"64"},{"range":"66","text":"64"},[1250,1252],"[dispatch]",[392,398],"[history, user]",[620,626],[461,467]]